{
  "name": "blinkit-store-placement-analysis",
  "version": "1.0.0",
  "description": "Dynamic website for Blinkit store placement analysis using real-time data and heatmaps",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "keywords": [
    "blinkit",
    "store-placement",
    "heatmap",
    "real-time",
    "kafka",
    "mern"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.5.0",
    "cheerio": "^1.1.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "kafkajs": "^2.2.4",
    "mongoose": "^7.5.0",
    "morgan": "^1.10.0",
    "puppeteer": "^24.17.1",
    "socket.io": "^4.7.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "nodemon": "^3.0.1"
  }
}
