# Server Configuration
PORT=5000
NODE_ENV=development

# MongoDB Connection
MONGODB_URI=mongodb://localhost:27017/blinkit-analysis

# Kafka Configuration
KAFKA_BROKERS=localhost:9092
KAFKA_CLIENT_ID=blinkit-analysis-client
KAFKA_GROUP_ID=blinkit-analysis-group

# Blinkit API (you'll need to get real API keys)
BLINKIT_API_KEY=your_blinkit_api_key
BLINKIT_API_URL=https://api.blinkit.com

# Competitor APIs (Zepto/Swiggy Instamart)
ZEPTO_API_KEY=your_zepto_api_key
ZEPTO_API_URL=https://api.zepto.in
SWIGGY_API_KEY=your_swiggy_api_key
SWIGGY_API_URL=https://api.swiggy.com

# Google Maps API
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# JWT Secret
JWT_SECRET=your_jwt_secret_key_here
