# Server Configuration
PORT=5000
NODE_ENV=development

# MongoDB Connection (no authentication)
MONGODB_URI=mongodb://localhost:27017/blinkit-analysis

# Kafka Configuration
KAFKA_BROKERS=localhost:9092
KAFKA_CLIENT_ID=blinkit-analysis-client
KAFKA_GROUP_ID=blinkit-analysis-group
KAFKAJS_NO_PARTITIONER_WARNING=1

# Real-Time Data Scraping (No API Keys Needed)
SCRAPING_ENABLED=true
SCRAPING_INTERVAL=10000
SCRAPING_DELAY=1000

# Target URLs for Real-Time Data Collection
BLINKIT_BASE_URL=https://blinkit.com
ZEPTO_BASE_URL=https://zepto.in
SWIGGY_INSTAMART_URL=https://swiggy.com/instamart

# User Agent for Web Scraping
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36

# Data Simulation for Testing
SIMULATION_ENABLED=true
SIMULATION_INTERVAL=5000

# JWT Secret
JWT_SECRET=blinkit_warehouse_analysis_secret_2024